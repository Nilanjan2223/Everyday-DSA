 for(int i = 0; i < n; i++){
            sum += arr[i];
            while(sum > s && i > left && left < n){
                sum -= arr[left];
                left++;
            }
            if(s == sum){
            ans.add(left+1);
            ans.add(i+1);
            break;
            }
        }
        if(s != sum){
            ans.add(-1);
        }
        
        
        return ans;
    }
}
